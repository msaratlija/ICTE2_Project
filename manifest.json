{
  "name": "Consent Management",
  "version": "1.1",
  "description": "Consent Management Extension.",
  "manifest_version": 2,
  "permissions": [
    "https://*/",
    "http://*/",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_matches": [
        "http://127.0.0.1:8000/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["static/js/jquery-3.4.1.js","endpoint_urls.js","background.js"],
    "persistent": false
  }
}